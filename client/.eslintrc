{
  "root": true,
  "extends": ["airbnb", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "plugins": ["prettier", "flowtype"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "import/prefer-default-export": 0,
    "prettier/prettier": [
      "error",
      { "singleQuote": true, "trailingComma": "all" }
    ]
  }
}
